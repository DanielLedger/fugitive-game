{
  "name": "me.DanL.FugitiveApp",
  "displayName": "FugitiveApp",
  "version": "2.0.0",
  "description": "The app for the game Fugitive.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "update-cfgmgen": "wget -O www/js/cfgmgen.js https://raw.githubusercontent.com/DanielLedger/ConfigMenuGenerator/master/cfgmgen.js",
    "update": "npm run update-cfgmgen && npm install"
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "Apache Cordova Team",
  "license": "Apache-2.0",
  "devDependencies": {
    "cordova-android": "^10.1.1",
    "cordova-background-geolocation-plugin": "^2.0.2",
    "cordova-browser": "^6.0.0",
    "cordova-plugin-android-permissions": "^1.1.2",
    "cordova-plugin-customurlscheme": "^5.0.2",
    "cordova-plugin-device": "^2.1.0",
    "cordova-plugin-whitelist": "^1.3.4",
    "cordova-plugin-x-socialsharing": "^6.0.3",
    "es6-promise-plugin": "^4.2.2"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-android-permissions": {},
      "cordova-plugin-customurlscheme": {
        "URL_SCHEME": "fugitive",
        "ANDROID_SCHEME": " ",
        "ANDROID_HOST": " ",
        "ANDROID_PATHPREFIX": "/"
      },
      "cordova-plugin-x-socialsharing": {},
      "cordova-plugin-device": {}
    },
    "platforms": [
      "browser",
      "android"
    ]
  },
  "dependencies": {
    "jquery": "^3.6.0"
  }
}